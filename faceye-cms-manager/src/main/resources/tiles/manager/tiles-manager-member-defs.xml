<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:cms 业务对像:Member 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- Member 管理 首页(列表页) -->
	<definition name="cms.member.manager" extends="default.cms.manager">
	    <put-attribute name="default-center" value="cms.member.center"></put-attribute>
	</definition>
	<definition name="cms.member.center" extends="default.cms.center">
	    <put-attribute name="default-center-right" value="/component/cms/member/member_center.jsp"></put-attribute>
	</definition>
	
	<!-- Member 新增,编辑页面 -->
	<definition name="cms.member.update" extends="default.cms.manager">
	    <put-attribute name="default-center" value="cms.member.update.center"></put-attribute>
	</definition>
	<definition name="cms.member.update.center" extends="default.cms.center">
	    <put-attribute name="default-center-right" value="/component/cms/member/member_update.jsp"></put-attribute>
	</definition>
	
	<!-- Member 明细页 -->
	<definition name="cms.member.detail" extends="default.cms.manager">
	    <put-attribute name="default-center" value="cms.member.detail.center"></put-attribute>
	</definition>
	<definition name="cms.member.detail.center" extends="default.cms.center">
	    <put-attribute name="default-center-right" value="/component/cms/member/member_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>
