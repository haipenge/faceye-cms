<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:activity 业务对像:LuckItem 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- LuckItem 管理 首页(列表页) -->
	<definition name="activity.luckItem.web.home" extends="default.web">
	    <put-attribute name="default-center" value="activity.luckItem.center"></put-attribute>
	</definition>
	<definition name="activity.luckItem.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/activity/luckItem/luckItem_center.jsp"></put-attribute>
	</definition>
	
	<!-- LuckItem 新增,编辑页面 -->
	<definition name="activity.luckItem.web.update" extends="default.web">
	    <put-attribute name="default-center" value="activity.luckItem.update.web.center"></put-attribute>
	</definition>
	<definition name="activity.luckItem.update.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/activity/luckItem/luckItem_update.jsp"></put-attribute>
	</definition>
	
	<!-- LuckItem 明细页 -->
	<definition name="activity.luckItem.web.detail" extends="default.web">
	    <put-attribute name="default-center" value="activity.luckItem.detail.web.center"></put-attribute>
	</definition>
	<definition name="activity.luckItem.detail.web.center" extends="default.web.center">
	    <put-attribute name="default-center-right" value="/component/activity/luckItem/luckItem_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>