<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:cms 业务对像:Category 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- Category 管理 首页(列表页) -->
	<definition name="cms.category.mobile.home" extends="default.mobile">
	    <put-attribute name="default-center" value="/component/cms/category/category_center.jsp"></put-attribute>
	</definition>
	<!-- Category 新增,编辑页面 -->
	<definition name="cms.category.mobile.update" extends="default.simple">
	     <put-attribute name="default-center" value="/component/cms/category/category_update.jsp"></put-attribute>
	</definition>
	
	<!-- Category 明细页 -->
	<definition name="cms.category.mobile.detail" extends="default.simple">
	    <put-attribute name="default-center" value="/component/cms/category/category_detail.jsp"></put-attribute>
	</definition>
	
	
</tiles-definitions>